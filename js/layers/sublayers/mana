upgrades: {
        11: {
            title: "Multiplier Upgrade 1",
            description: "Double cash gain.",
            cost: new Decimal("50"),
            currencyDisplayName: "cash",
            currencyInternalName: "points",
        },
        12: {
            title: "Multiplier Upgrade 2",
            description: "Slightly boost multiplier based on cash.",
            cost: new Decimal("100"),
            currencyDisplayName: "cash",
            currencyInternalName: "points",
            effect() {
                return player.points.add("1").pow("0.05")
            },
            effectDisplay() { return format(upgradeEffect(this.layer, this.id))+"x" },
            unlocked() { return hasUpgrade("p", 11) },
        },
        13: {
            title: "Multiplier Upgrade 3",
            description: "Boost the Multiplier effect based on Cash. (Affected by Softcap)",
            cost: new Decimal("500"),
            currencyDisplayName: "cash",
            currencyInternalName: "points",
            effect() {
                return player.points.add("1").pow("0.3")
            },
            effectDisplay() { return format(upgradeEffect(this.layer, this.id))+"x" },
    },
})